.head-north {overflow: hidden; height: 71px;background: #2076C3 url(../images/main/headbg.jpg) repeat-x; line-height: 64px;color: #fff;font-family: Verdana, 微软雅黑,黑体}
.head-south {height: 30px; background: #D2E0F2;}
.head-left {background:url(../images/main/logo.png) no-repeat left; margin-left:10px; width:310px; height:64px;float:left; padding-left:80px; font-size:30px; font-weight:bolder; font-family:'Microsoft YaHei' !important;}
.head-right{float:right; padding-right:10px;}
.head-right a{color:white;}
.head-right b{font-size:12px; font-family:Verdana,宋体,Tahoma;}

.tabs-tool {height:auto !important;}
.tabs-tool{border-right:0 !important;}

.layout-split-proxy-h{
	position:absolute;
	width:2px;
	background:#888;
	font-size:1px;
	cursor:e-resize;
	display:none;
	z-index:5;
}

.layout-split-north{
	border-bottom:5px solid #efefef;
}
.layout-split-south{
	border-top:5px solid #efefef;
}
.layout-split-east{
	border-left:0px solid #efefef;
}
.layout-split-west{
	border-right:0px solid #efefef;
}
a:link {
 text-decoration: none;
}
a:visited {
 text-decoration: none;
}
a:hover {
 text-decoration: underline;
}
a:active {
 text-decoration: none;
}

.cs-west {
	width:200px;padding:0px;
}



.ui-skin-nav {
    float: right;
	padding: 0;
	margin-right: 10px;
	list-style: none outside none;
	height: 20px;
}

.ui-skin-nav .li-skinitem {
    float: left;
    font-size: 12px;
    line-height: 30px;
	margin-left: 10px;
    text-align: center;
}
.ui-skin-nav .li-skinitem span {
	cursor: pointer;
	width:10px;
	height:10px;
	display:inline-block;
}
.ui-skin-nav .li-skinitem span.cs-skin-on{
	border: 1px solid #FFFFFF;
}

.ui-skin-nav .li-skinitem span.gray{background-color:gray;}
.ui-skin-nav .li-skinitem span.default{background-color:blue;}
.ui-skin-nav .li-skinitem span.bootstrap{background-color:#D7EBF9;}
.ui-skin-nav .li-skinitem span.black{background-color:black;}
.ui-skin-nav .li-skinitem span.metro{background-color:#FFE57E;}


.ui-time-nav{
    float: right;
	padding: 0;
	margin-top: 5px;
	margin-right: 10px;
	list-style: none outside none;
	height: 20px;
}

.ui-time-nav .li-timeitem {
    float: left;
    font-size: 12px;
    line-height: 30px;
	margin-left: 10px;
    text-align: center;
}

.ui-time-nav .li-timeitem span 
{height:30px;float:left;display:inline-block;}

#operator { float:left; margin-left:10px;margin-top:12px;}

/*主界面代码结束*/