package com.domain;

import java.util.Date;

public class Weather {
	
	private int id; //城市id 自增长
	private String cityname;//城市名
	private String wtime;//日期和星期
	private String weather;//天气
	private String htem;//高温
	private String ltem;//低温
	private String feel;//舒适度
	private int taskid;
	private String date;
	
	public String getDate() {
		return date;
	}
	public void setDate(String date) {
		this.date = date;
	}
	public int getTaskid() {
		return taskid;
	}
	public String getWtime() {
		return wtime;
	}
	public void setWtime(String wtime) {
		this.wtime = wtime;
	}
	public void setTaskid(int taskid) {
		this.taskid = taskid;
	}
	public Weather() {
		super();
	}
	public Weather(String cityname, String weather, String htem, String ltem, String feel,int taskid) {
		super();
		this.cityname = cityname;
		this.weather = weather;
		this.htem = htem;
		this.ltem = ltem;
		this.feel = feel;
		this.taskid = taskid;
	}
	public Weather(String cityname, String weather, String htem, String ltem, String feel) {
		super();
		this.cityname = cityname;
		this.weather = weather;
		this.htem = htem;
		this.ltem = ltem;
		this.feel = feel;
	}
	
	public int getId() {
		return id;
	}
	@Override
	public String toString() {
		return cityname + "天气，" + "今天是" + wtime + "，" + weather 
				+ "，"  + htem  + "，" + ltem + "，" + feel+"。";
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getCityname() {
		return cityname;
	}
	public void setCityname(String cityname) {
		this.cityname = cityname;
	}
	public String getWeather() {
		return weather;
	}
	public void setWeather(String weather) {
		this.weather = weather;
	}
	public String getHtem() {
		return htem;
	}
	public void setHtem(String htem) {
		this.htem = htem;
	}
	public String getLtem() {
		return ltem;
	}
	public void setLtem(String ltem) {
		this.ltem = ltem;
	}
	public String getFeel() {
		return feel;
	}
	public void setFeel(String feel) {
		this.feel = feel;
	}
	

	
	
}
